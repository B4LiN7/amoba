{
  "name": "amoba",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start": "cd backend && nest start",
    "start:dev": "cd backend && nest start --watch",
    "start:debug": "cd backend && nest start --debug --watch",
    "start:prod": "cd backend && node dist/main",
    "build": "cd backend && nest build",
    "format": "cd backend && prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "cd backend && eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cd backend && jest",
    "test:watch": "cd backend && jest --watch",
    "test:cov": "cd backend && jest --coverage",
    "test:debug": "cd backend && node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cd backend && jest --config ./test/jest-e2e.json",
    "prisma:generate": "cd backend && pnpm prisma generate",
    "prisma:migrate": "cd backend && pnpm prisma migrate dev init",
    "prisma:reset": "cd backend && pnpm prisma migrate reset --force",
    "dev": "cd frontend && vite",
    "build:frontend": "cd frontend && tsc && vite build",
    "lint:frontend": "cd frontend && eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "cd frontend && vite preview"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.7",
    "@nestjs/core": "^10.3.7",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/platform-express": "^10.3.7",
    "@nestjs/schedule": "^4.0.1",
    "@prisma/client": "^5.12.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "express-session": "^1.18.0",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.7",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.5",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "axios": "^1.6.8",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "prisma": "^5.12.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.22.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.4",
    "vite": "^5.2.8"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "schema": "backend/prisma/schema.prisma"
  }
}